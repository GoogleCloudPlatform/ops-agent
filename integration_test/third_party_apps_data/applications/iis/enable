# Replace {$version} with '1' or '2'

Add-Content 'C:\Program Files\Google\Cloud Operations\Ops Agent\config\config.yaml' "
metrics:
  receivers:
    iis_v{$version}:
      type: iis
      receiver_version: ($version}
  service:
    pipelines:
      iispipeline:
        receivers:
          - iis_v{$version}
logging:
  receivers:
    iis_access:
      type: iis_access
  service:
    pipelines:
      iis:
        receivers:
        - iis_access
"

Stop-Service google-cloud-ops-agent -Force
Start-Service google-cloud-ops-agent* 
