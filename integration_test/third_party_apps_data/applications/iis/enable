Add-Content 'C:\Program Files/Google\Cloud Operations\Ops Agent\config\config.yaml' "
logging:
  receivers:
    iis_access:
      type: iis_access
  service:
    pipelines:
      iis:
        receivers:
        - iis_access
"

Stop-Service google-cloud-ops-agent -Force
Start-Service google-cloud-ops-agent* 