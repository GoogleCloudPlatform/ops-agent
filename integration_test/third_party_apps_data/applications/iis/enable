Add-Content 'C:\Program Files\Google\Cloud Operations\Ops Agent\config\config.yaml' "
logging:
  receivers:
    iis_access:
      type: iis_access
  service:
    pipelines:
      iis:
        receivers:
        - iis_access
metrics:
  receivers:
    iis_v2:
      type: iis
      receiver_version: "2"
  service:
    pipelines:
      iispipeline:
        receivers:
          - iis_v2
"

Stop-Service google-cloud-ops-agent -Force
Start-Service google-cloud-ops-agent* 